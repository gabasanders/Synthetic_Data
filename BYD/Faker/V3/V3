  {
   "cell_type": "markdown",
   "id": "a9b9b16c-19f5-4f43-b959-13f856da59fd",
   "metadata": {},
   "source": [
    "# Main Table"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "b61c6e1c-b642-4e94-8af3-b8526a7ed5ec",
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "def generate_main_table(base_table):\n",
    "    data = pd.DataFrame()\n",
    "\n",
    "    count = 0\n",
    "    \n",
    "    for i, row in base_table.iterrows():\n",
    "        count = i\n",
    "        createDate = faker.date_this_year()\n",
    "        updateDate = faker.date_between(start_date=createDate)\n",
    "\n",
    "        dominio = random.choice([\"@gmail.com\",\"@outlook.com\",\"@yahoo.com.br\",\"uol.com.br\"])\n",
    "        email = f\"{row['CLIENT_LAST_NAME']}.{row['CLIENT_FIRST_NAME']}@{dominio}\"\n",
    "\n",
    "\n",
    "        data.loc[i, 'DEALER_ORDER_ID'] = row['dealerOrderId']\n",
    "        data.loc[i, 'DEALER_ORDER_VERSION'] = '1'\n",
    "        data.loc[i, 'DEALER_CODE'] = row['dealerId']\n",
    "        data.loc[i, 'EXTERNAL_DEALER_ORDER_NO'] = randint(20,120)    #Inconsistent for now, it isnt tracking each dealer's order count\n",
    "        data.loc[i, 'SERVICE_CONSULTANT'] = faker.name()\n",
    "        data.loc[i, 'CREATE_BY'] = '9999999999'\n",
    "        #data.loc[i, 'createName'] = 'Name'\n",
    "        data.loc[i, 'CREATE_DATE'] = createDate\n",
    "        data.loc[i, 'UPDATE_BY'] = '9999999988'\n",
    "        #data.loc[i, 'updateName'] = 'Name'\n",
    "        data.loc[i, 'UPDATE_DATE'] = updateDate\n",
    "        data.loc[i, 'REPAIR_TYPE'] = random.choice(['Warranty', 'Non-Warranty'])\n",
    "        data.loc[i, 'REPAIR_SOURCE'] = \"99301002\" #External\n",
    "        data.loc[i, 'ORDER_STATUS'] = random.choice(['Opened']) #(['Opened', 'Closed', 'In Progress'])\n",
    "        data.loc[i, 'ORDER_TYPE'] = row['orderType']\n",
    "        data.loc[i, 'ORDER_CREATE_DATE'] = createDate\n",
    "        data.loc[i, 'ORDER_LAST_BALANCE_DATE'] = updateDate\n",
    "        data.loc[i, 'PREVIOUS_LAST_REPAIR_DATE'] = \"1800-01-01 00:00:00\"\n",
    "        data.loc[i, 'VIN'] = faker.unique.license_plate()\n",
    "        #data.loc[i, 'custName'] = f\"{row['CLIENT_FIRST_NAME']} {row['CLIENT_LAST_NAME']}\"\n",
    "        #data.loc[i, 'custEmail'] = email\n",
    "        data.loc[i, 'DELIVERER'] = faker.name()\n",
    "        data.loc[i, 'DELIVERER_MOBILE'] = faker.phone_number()\n",
    "        data.loc[i, 'ENTER_SHOP_DATE'] = createDate\n",
    "        data.loc[i, 'DISPATCH_DATE'] = faker.date_between(start_date=createDate,end_date=updateDate)\n",
    "        data.loc[i, 'COMPLETION_DATE'] = updateDate\n",
    "        data.loc[i, 'ETD'] = faker.date_between(start_date=createDate,end_date=updateDate)\n",
    "        data.loc[i, 'POWER_MILEAGE'] = faker.random_int(min=0, max=50000)\n",
    "        data.loc[i, 'TOTAL_MILEAGE'] = faker.random_int(min=0, max=100000)\n",
    "        data.loc[i, 'CREATE_CLAIM_FLAG'] = random.choice(['Y', 'N'])\n",
    "        data.loc[i, 'CLAIM_NO'] = faker.unique.uuid4() if data.loc[i, 'CREATE_CLAIM_FLAG'] == 'Y' else None\n",
    "        data.loc[i, 'ACTIVITY_FLAG'] = random.choice(['Y', 'N'])\n",
    "        data.loc[i, 'ACTIVITY_CODE'] = faker.unique.uuid4() if data.loc[i, 'ACTIVITY_FLAG'] == 'Y' else None\n",
    "        data.loc[i, 'TROUBLE_DESC'] = row['CLAIM']\n",
    "        data.loc[i, 'DELIVER_PROBLEM'] = None\n",
    "        data.loc[i, 'CHECK_RESULT'] = None\n",
    "        data.loc[i, 'CURRENCY'] = 'BRL'\n",
    "        data.loc[i, 'TOTAL_PAID_IN_AMOUNT'] = round(random.uniform(100, 3000), 2)\n",
    "\n",
    "\n",
    "    for new_row in range(count + 1, count + round(NUMBER_OF_REQUESTS*0.3)):\n",
    "        random_id = random.sample(range(NUMBER_OF_REQUESTS), NUMBER_OF_REQUESTS) #Chooses a random Order ID to generate a second version\n",
    "        new_row = data.iloc[random_id].copy()\n",
    "        new_row['DEALER_ORDER_VERSION'] = '2'\n",
    "        new_row['ORDER_STATUS'] = random.choice(['Completed']*10 + ['Invalid']*3)\n",
    "        data = data._append(new_row, ignore_index=True)\n",
    "\n",
    "    return data"
   ]
  },
