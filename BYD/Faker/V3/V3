    "def generate_parts(base_table):\n",
    "    \n",
    "    data = pd.DataFrame()\n",
    "\n",
    "    \n",
    "    additional_parts = []\n",
    "\n",
    "    for i, row in base_table.iterrows():\n",
    "\n",
    "        data.loc[i, 'REPAIR_PART_ID '] = f\"{(i):010}\"\n",
    "        data.loc[i, 'REPAIR_PART_VERSION'] = row['SALE_PART_VERSION']\n",
    "        data.loc[i, 'DEALER_ORDER_ID'] = row['DEALER_ORDER_ID']\n",
    "        data.loc[i, 'DEALER_ORDER_VERSION'] = row['SALE_PART_VERSION']\n",
    "        data.loc[i, 'PART_CODE'] = row['PART_CODE']\n",
    "        data.loc[i, 'PART_NAME'] = row['PART_NAME']\n",
    "        data.loc[i, 'PART_QUANTITY'] = row['PART_QUANTITY']\n",
    "        data.loc[i, 'SN_OF_UNINSTALLED_PART'] = f\"{randint(50,100):010}\"\n",
    "        data.loc[i, 'SN_OF_INSTALLED_PART'] = f\"{randint(50,100):010}\"\n",
    "        data.loc[i, 'REPLACEMENT_DATE'] = faker.date_between(row['CREATE_DATE'])\n",
    "        data.loc[i, 'COST'] =  round(random.uniform(100, 1000), 2)\n",
    "        data.loc[i, 'RETAIL_PRICE'] =  round(random.uniform(100, 1000), 2)\n",
    "        data.loc[i, 'DISCOUNT_RATE'] =  round(random.uniform(100, 1000), 2)\n",
    "        data.loc[i, 'PAID_IN_AMOUNT'] =  round(random.uniform(100, 5000), 2)\n",
    "        data.loc[i, 'MAIN_PART_FLAG'] = f\"{randint(1000,1000000):08}\"\n",
    "        data.loc[i, 'CREATE_BY'] = '9999999999'\n",
    "        data.loc[i, 'CREATE_DATE'] = row['CREATE_DATE']\n",
    "        data.loc[i, 'UPDATE_BY'] = '9999999988'\n",
    "        data.loc[i, 'UPDATE_DATE'] = faker.date_between(start_date= row['CREATE_DATE'])\n",
    "\n",
    "    return data\n"
   ]
