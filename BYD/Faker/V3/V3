   "cell_type": "markdown",
   "id": "243a5a3e-1d83-4c9a-bc1d-8275655f2612",
   "metadata": {},
   "source": [
    "# Dealer Info Table"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "5ed9c5dc-9dd7-4478-9cf8-74b52bfa3d60",
   "metadata": {},
   "outputs": [],
   "source": [
    "regions_and_states = {\n",
    "    'Brazil North': {\n",
    "        'Amazonas': ['Manaus', 'Parintins', 'Itacoatiara']\n",
    "    },\n",
    "    'Brazil Northeast': {\n",
    "        'Bahia': ['Salvador'],\n",
    "        'Ceará': ['Fortaleza', 'Juazeiro do Norte', 'Sobral']\n",
    "    },\n",
    "    'Brazil Central-West': {\n",
    "        'Distrito Federal': ['Brasília'],\n",
    "        'Goiás': ['Goiânia'],\n",
    "        'Mato Grosso': ['Cuiabá', 'Rondonópolis', 'Sinop']\n",
    "    },\n",
    "    'Brazil Southeast': {\n",
    "        'Espírito Santo': ['Vila Velha', 'Vitória'],\n",
    "        'Minas Gerais': ['Belo Horizonte', 'Juiz de Fora', 'Uberlândia'],\n",
    "        'São Paulo': ['São Paulo', 'Campinas', 'Santos', 'São José dos Campos', 'Jundiaí', 'Ribeirão Preto']\n",
    "    },\n",
    "    'Brazil South': {\n",
    "        'Paraná': ['Curitiba', 'Londrina', 'Maringá'],\n",
    "        'Rio Grande do Sul': ['Porto Alegre', 'Caxias do Sul', 'Pelotas', 'Passo Fundo']\n",
    "    }\n",
    "}\n",
    "\n",
    "def generate_dealers(x):\n",
    "    data = pd.DataFrame()\n",
    "\n",
    "    for i in range(x):\n",
    "\n",
    "        region = random.choices(list(regions_and_states.keys()),weights=[3,2,2,5,2],k=1)[0]\n",
    "        \n",
    "        if (region == 'Brazil Southeast'):\n",
    "            state = random.choices(list(regions_and_states[region].keys()),weights=[1,2,5,],k=1)[0]\n",
    "        else:\n",
    "            state = random.choice(list(regions_and_states[region].keys()))\n",
    "            \n",
    "        city = random.choice(regions_and_states[region][state])\n",
    "        \n",
    "        data.loc[i, 'DEALER_CODE'] = f\"{(i + 1):06}\"  # Dealer ID with 4 digits\n",
    "        data.loc[i, 'DEALER_NAME'] = faker.unique.company()#f\"{faker.first_name()} {faker.last_name()}\"\n",
    "        data.loc[i, 'PASSED_TRAINING'] = random.choice(['Y']*7 + ['N']*3)  #70% chance of passing the  training\n",
    "        data.loc[i, 'CONTACT_NUMBER'] = faker.msisdn()\n",
    "        data.loc[i, 'COUNTRY'] = 'Brazil'\n",
    "        #data.loc[i, 'REGION'] = region\n",
    "        data.loc[i, 'STATE'] = state\n",
    "        data.loc[i, 'CITY'] = city\n",
    "        data.loc[i, 'ADDRESS'] = faker.address().replace('\\n', ' ')  # Replace newline with space for better formatting\n",
    "\n",
    "    return data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "f732f5ab-7372-4063-9d8b-f65bf803bc35",
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "text/html": [
       "<div>\n",
       "<style scoped>\n",
       "    .dataframe tbody tr th:only-of-type {\n",
       "        vertical-align: middle;\n",
       "    }\n",
       "\n",
       "    .dataframe tbody tr th {\n",
       "        vertical-align: top;\n",
       "    }\n",
       "\n",
       "    .dataframe thead th {\n",
       "        text-align: right;\n",
       "    }\n",
       "</style>\n",
       "<table border=\"1\" class=\"dataframe\">\n",
       "  <thead>\n",
       "    <tr style=\"text-align: right;\">\n",
       "      <th></th>\n",
       "      <th>DEALER_CODE</th>\n",
       "      <th>DEALER_NAME</th>\n",
